".text.html":
  "polymer element":
    "prefix": "pe"
    "body": """
    <dom-module id="$1">
      <template>
        <style>
          :host {
            display: block;
          }
        </style>
        $2
      </template>
      <script>
        Polymer({
          is: '$1'
        });
      </script>
    </dom-module>
    """
